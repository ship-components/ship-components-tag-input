{
  "name": "ship-components-tag-input",
  "version": "0.1.0",
  "description": "Material Design Input and Typeahead for Tags (or \"Chips\" in Material Design speak)",
  "main": "src/TagInput.js",
  "scripts": {
    "start": "grunt server",
    "build": "npm test && grunt clean && grunt build",
    "lint": "./node_modules/eslint/bin/eslint.js src/",
    "test": "grunt lint && ./node_modules/jest-cli/bin/jest.js --coverage",
    "watch:test": "./node_modules/jest-cli/bin/jest.js --coverage --watch",
    "coverage": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js -v",
    "update:packages": "node ./lib/wipe-dependencies.js && rm -rf node_modules && npm update --save-dev && npm update --save"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ship-components/ship-components-tag-input.git"
  },
  "keywords": [
    "react",
    "select",
    "es2015",
    "javascript",
    "javascript-library",
    "grunt",
    "webpack",
    "jest"
  ],
  "author": "Sepand Assadi <sepand.assadi@sony.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ship-components/ship-components-tag-input/issues"
  },
  "homepage": "https://github.com/ship-components/ship-components-tag-input#readme",
  "dependencies": {
    "classnames": "^2.2.5",
    "fuse.js": "^3.0.5",
    "lodash.clonedeep": "^4.5.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "ship-components-icon": "^0.1.2",
    "ship-components-outsideclick": "^0.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-es2015-classes": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "coveralls": "^2.13.1",
    "css-loader": "^0.28.4",
    "eslint": "^4.1.1",
    "eslint-config-ship-components": "^1.4.2",
    "eslint-loader": "^1.8.0",
    "eslint-plugin-react": "^7.1.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "grunt": "^1.0.1",
    "grunt-contrib-clean": "^1.1.0",
    "grunt-coveralls": "^1.0.1",
    "grunt-eslint": "^20.0.0",
    "grunt-webpack": "^3.0.0",
    "jest-cli": "^20.0.4",
    "lodash": "^4.17.4",
    "object-assign": "^4.1.1",
    "package-banner": "^0.1.1",
    "postcss-calc": "^6.0.0",
    "postcss-color-function": "^4.0.0",
    "postcss-color-hex-alpha": "^3.0.0",
    "postcss-loader": "^2.0.6",
    "postcss-nested": "^2.0.2",
    "postcss-simple-vars": "^4.0.0",
    "prop-types": "^15.5.10",
    "sockjs-client": "^1.1.4",
    "style-loader": "^0.18.2",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.5.0"
  },
  "jest": {
    "transform": "node_modules/babel-jest",
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "es6"
    ],
    "unmockedModulePathPatterns": [
      "node_modules/react",
      "node_modules/react-dom"
    ]
  }
